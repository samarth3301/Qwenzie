generator client {
  provider             = "prisma-client-py"
  recursive_type_depth = 5
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model guildConfig {
  id           String @id @default(uuid()) @map("_id")
  guildId      BigInt @unique
  prefix       String @default("?")
  pannel_count Int    @default(0)
}
